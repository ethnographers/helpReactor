{
  "name": "helpreactor",
  "version": "1.0.0",
  "description": "",
  "engines": {
    "node": "^6.11.0"
  },
  "scripts": {
    "react-dev": "webpack -d --watch",
    "server-dev": "nodemon server/index.js",
    "start": "node ./server/index.js",
    "postinstall": "webpack -p --config ./webpack.config.js --progress && node ./database/migrate.js",
    "db-setup": "npm run db-create && npm run db-start && npm run db-init",
    "db-create": "initdb database/db",
    "db-start": "pg_ctl -D database/db -l logfile start",
    "db-init": "createdb helpReactor",
    "db-stop": "pg_ctl -D database/db -l logfile stop",
    "db-migrate": "node database/migrate.js"
  },
